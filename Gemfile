source 'https://rubygems.org'

# GitHub Pages will use its own Jekyll version
# For local development, uncomment the jekyll gem
group :jekyll_plugins do
  # gem 'jekyll'
  gem 'jekyll-feed'
  gem 'jekyll-sitemap'
  gem 'jekyll-redirect-from'
  gem 'jemoji'
  gem 'webrick', '~> 1.8'
end

gem 'github-pages'
